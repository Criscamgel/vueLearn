 value >= 0\n            }\n        }\n    },\n    // name: 'Patito'\n    data() {\n        return {\n            counter: this.start\n        }\n    },\n    methods: {\n        getSquareValue() {\n            return this.counter * this.counter\n        },\n        increase() {\n            this.counter++\n        },\n        decrease() {\n            this.counter--\n        },\n    },\n    computed: {\n        squareCounter() {\n            return this.counter * this.counter\n        },\n        customTitle() {\n            return this.title || 'Counter'\n        }\n    }\n}\n</script>\n\n<style>\n\nbutton{\n    background-color: #64B687;\n    border-radius: 5px;\n    border: 1px solid white;\n    color: white;\n    cursor: pointer;\n    margin: 0 5px;\n    padding: 5px 15px;\n    transition: 0.3s ease-in-out;\n}\n\nbutton:hover {\n    background-color: #5aa67b;\n    transition: 0.3s ease-in-out;\n}\n\n</style>"]}]}